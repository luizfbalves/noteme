model Note {
  id          String    @id @default(uuid())
  description String?
  createdAt   DateTime  @default(now())
  updatedAt   DateTime? @default(now())
  userId      String

  @@map("notes")
}

model users {
  id        String    @id @default(uuid())
  email     String    @unique
  name      String
  password  String
  createdAt DateTime  @default(now())
  updatedAt DateTime? @default(now())

  @@map("users")
}
